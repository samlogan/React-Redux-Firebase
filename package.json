    "express": "^4.16.3",
    "firebase": "^5.3.0",
    "firebase-admin": "^5.13.1",
    "helmet": "^3.13.0",
    "method-override": "^2.3.9",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
