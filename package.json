    "eslint-plugin-react": "^7.1.0",
    "import-glob-loader": "^1.1.0",
    "nodemon": "^1.10.2",
    "null-loader": "^0.1.1",
    "react-addons-test-utils": "^15.4.1",
    "react-transform-catch-errors": "^1.0.2",
    "webpack-cli": "^2.0.10"
  },
  "engines": {
    "node": "6.x||7.x||8.x||9.x"
  },
  "dependencies": {
    "axios": "^0.16.2",
